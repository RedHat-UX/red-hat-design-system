{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "downlevelIteration": true,
    "emitDeclarationOnly": false,
    "experimentalDecorators": true,
    "importHelpers": true,
    "incremental": true,
    "inlineSources": true,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "verbatimModuleSyntax": true,
    "noEmitOnError": false,
    "rootDir": ".",
    "sourceMap": true,
    "strict": true,
    "target": "es2020",
    "skipLibCheck": true,
    "useDefineForClassFields": false
  }
}
