+            const TAG = tag ?? `${this.constructor.name}-${key}`;
+            const START_TAG = `start-${TAG}`;
+            const END_TAG = `end-${TAG}`;
+            performance.mark(START_TAG);
+            const x = f.call(this, ...args);
+            const ret = () => {
+                performance.mark(END_TAG);
