+
+  #initListbox() {
+    this.#mo.disconnect();
+    this.#listbox?.removeEventListener('focusout', this.#onFocusoutListbox);
+    this.#listbox?.removeEventListener('keydown', this.#onKeydownListbox);
+    this.#listbox?.removeEventListener('click', this.#onClickListbox);
