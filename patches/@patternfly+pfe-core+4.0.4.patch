+  /** The crosswise alignment of the invoker on which to display the floating DOM */
+  get alignment(): Alignment {
+    return this.#alignment ?? 'center';
+  }
+
+  /** The side of the invoker on which to display the floating DOM */
+  get anchor(): Anchor {
+    return this.#anchor ?? '';
+  }
