+  content: Lazy<HTMLElement>;
+  invoker?: Lazy<HTMLElement>;
+  arrow?: Lazy<HTMLElement>;
+  shift?: boolean;
+  padding?: number;
+  fallbackPlacements?: Placement[];
+}
+
+interface ShowOptions {
+  offset?: Offset;
+  placement?: Placement;
+  flip?: boolean;
+  fallbackPlacements?: Placement[];
+}
